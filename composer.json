{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "beyondcode/laravel-websockets": "^1.1",
        "coderello/laravel-passport-social-grant": "^2.0",
        "dg/rss-php": "^1.3",
        "dingo/api": "^2.1",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.2",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "tymon/jwt-auth": "dev-develop#f72b8eb as 1.0.0-rc.3.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.3.16",
        "phploc/phploc": "^5.0",
        "phpunit/phpunit": "^7.5",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cs-fix": [
            "./vendor/bin/php-cs-fixer fix app/ --rules=@PSR2",
            "./vendor/bin/php-cs-fixer fix database/ --rules=@PSR2",
            "./vendor/bin/php-cs-fixer fix routes/ --rules=@PSR2"
        ],
        "analyse": [
            "@php artisan code:analyse --level=max"
        ],
        "gen-doc": [
            "@php ./qa/phpDocumentor.phar -d app -t ./qa/doc"
        ],
        "php-cs": [
            "./vendor/bin/phpcs"
        ],
        "phpcbf": [
            "./vendor/bin/phpcbf"
        ],
        "phpmd": [
            "@php ./qa/phpmd.phar app text cleancode,codesize --reportfile ./qa/phpmd.html"
        ],
        "phpunit-coverage-html": [
            "./vendor/bin/phpunit --coverage-html ./qa/coverage-html"
        ],
        "phpunit": [
            "./vendor/bin/phpunit"
        ],
        "phploc": [
            "./vendor/bin/phploc app"
        ],
        "phpcpd": [
            "./vendor/bin/phpcpd --fuzzy --log-pmd qa/phpcpd.xml app"
        ]
    }
}
